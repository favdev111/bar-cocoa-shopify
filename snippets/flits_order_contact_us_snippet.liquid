    <div class="flits-form-content">

      <!-- Form only for display purpose -->
      {%- assign formId = 'flits-contact-form' -%}
      {% form 'contact', id: formId %}
      <div class="flits-row">
        <div class="flits-col-sm-6 flits-col-md-6 flits-popup-mb-15">
          <div class="flits-input-wrap flits-input-icon-wrap">
            <label class="flits-input-label" for="{{ formId }}-first-name">{{ 'flits.order_contact_us.first_name_label' | t }}</label>
            <div class="flits-input-icon">
              <svg viewBox="0 0 24 24" width="18" height="18" stroke="currentColor" stroke-width="2" fill="none" stroke-linecap="round" stroke-linejoin="round" class="css-i6dzq1"><path d="M20 21v-2a4 4 0 0 0-4-4H8a4 4 0 0 0-4 4v2"></path><circle cx="12" cy="7" r="4"></circle></svg>
            </div>
            <input type="text" class="flits-input flits-form-first-name" id="{{ formId }}-first-name"
                   placeholder="{{ 'flits.order_contact_us.first_name_placeholder' | t }}"
                   name="contact[{{ 'flits.order_contact_us.first_name_label' | t }}]"
                   value="{% if form[first_name] %}{{ form[first_name] }}{% elsif customer %}{{ customer.first_name }}{% endif %}">
          </div>
        </div>
        <div class="flits-col-sm-6 flits-col-md-6 flits-popup-mb-15">
          <div class="flits-input-wrap flits-input-icon-wrap">
            <label class="flits-input-label" for="{{ formId }}-last-name">{{ 'flits.order_contact_us.last_name_label' | t }}</label>
            <div class="flits-input-icon">
              <svg viewBox="0 0 24 24" width="18" height="18" stroke="currentColor" stroke-width="2" fill="none" stroke-linecap="round" stroke-linejoin="round" class="css-i6dzq1"><path d="M20 21v-2a4 4 0 0 0-4-4H8a4 4 0 0 0-4 4v2"></path><circle cx="12" cy="7" r="4"></circle></svg>
            </div>
            <input type="text" class="flits-input flits-form-last-name" id="{{ formId }}-last-name"
                   placeholder="{{ 'flits.order_contact_us.last_name_placeholder' | t }}"
                   name="contact[{{ 'flits.order_contact_us.last_name_label' | t }}]"
                   value="{% if form[last_name] %}{{ form[last_name] }}{% elsif customer %}{{ customer.last_name }}{% endif %}">
          </div>
        </div>
      </div>

      <div class="flits-row">
        <div class="flits-col-sm-12 flits-col-md-6 flits-popup-mb-15">
          <div class="flits-input-wrap flits-input-icon-wrap">
            <label class="flits-input-label" for="{{ formId }}-email">{{ 'flits.order_contact_us.email_label' | t }} <span class ="flits-requird-sign" aria-hidden="true">*</span></label>
            <div class="flits-input-icon">
              <svg viewBox="0 0 24 24" width="18" height="18" stroke="currentColor" stroke-width="2" fill="none" stroke-linecap="round" stroke-linejoin="round" class="css-i6dzq1"><path d="M4 4h16c1.1 0 2 .9 2 2v12c0 1.1-.9 2-2 2H4c-1.1 0-2-.9-2-2V6c0-1.1.9-2 2-2z"></path><polyline points="22,6 12,13 2,6"></polyline></svg>
            </div>
            <input type="text"
                   placeholder="{{ 'flits.order_contact_us.email_placeholder' | t }}"
                   id="{{ formId }}-email"
                   name="contact[{{ 'flits.order_contact_us.email_label' | t }}]"
                   autocorrect="off"
                   autocapitalize="off"
                   value="{% if form.email %}{{ form.email }}{% elsif customer %}{{ customer.email }}{% endif %}"
                   aria-required="true"
                   class="flits-input flits-form-email">
          </div>
          <small id="flits-alert-email" class="flits-form-alert"></small>
        </div>
        <div class="flits-col-sm-12 flits-col-md-6 flits-popup-mb-15">
          <div class="flits-input-wrap flits-input-icon-wrap">
            <label class="flits-input-label" for="{{ formId }}-phone">{{ 'flits.order_contact_us.phone_number_label' | t }}</label>
            <div class="flits-input-icon">
              <svg viewBox="0 0 24 24" width="18" height="18" stroke="currentColor" stroke-width="2" fill="none" stroke-linecap="round" stroke-linejoin="round" class="css-i6dzq1"><path d="M22 16.92v3a2 2 0 0 1-2.18 2 19.79 19.79 0 0 1-8.63-3.07 19.5 19.5 0 0 1-6-6 19.79 19.79 0 0 1-3.07-8.67A2 2 0 0 1 4.11 2h3a2 2 0 0 1 2 1.72 12.84 12.84 0 0 0 .7 2.81 2 2 0 0 1-.45 2.11L8.09 9.91a16 16 0 0 0 6 6l1.27-1.27a2 2 0 0 1 2.11-.45 12.84 12.84 0 0 0 2.81.7A2 2 0 0 1 22 16.92z"></path></svg>
            </div>
            <input type="tel" readonly class="flits-input flits-contact-us-phone-country-code-textbox" value="">
            <input type="tel" class="flits-input flits-form-phone" id="{{ formId }}-phone"
                   placeholder="{{ 'flits.order_contact_us.phone_number_placeholder' | t }}"
                   name="contact[{{ 'flits.order_contact_us.phone_number_label' | t }}]"
                   value="{% if form[phone] %}{{ form[phone] }}{% elsif customer %}{{ customer.phone }}{% endif %}">
            <div class="flits-contact-us-phone-number-country">
              <span class="flits-contact-us-phone-number-country-flag"></span>
              <svg viewBox="0 0 24 24" width="15" height="15" stroke="currentColor" stroke-width="2" fill="none" stroke-linecap="round" stroke-linejoin="round" class="css-i6dzq1"><polyline points="6 9 12 15 18 9"></polyline></svg>
              <select></select>
            </div>
          </div>
          <small id="flits-alert-phone" class="flits-form-alert"></small>
        </div>
      </div>

      <div class="flits-row">
        <div class="flits-col-md-12 flits-popup-mb-15">
          <div class="flits-input-wrap">
            <label class="flits-input-label" for="{{ formId }}-reason">{{ 'flits.order_contact_us.reason_label' | t }} <span class ="flits-requird-sign" aria-hidden="true">*</span></label>
            <div class="flits-select-row">
              <select class="flits-input flits-form-reason" name="contact[{{ 'flits.order_contact_us.reason_label' | t }}]" id="{{ formId }}-reason">
                <option value="" disabled="" selected="">{{ 'flits.order_contact_us.select_any_reason' | t }}</option>
                <option value="{{ 'flits.order_contact_us.order_cancel_related_reason' | t }}">{{ 'flits.order_contact_us.order_cancel_related_reason' | t }}</option>
                <option value="{{ 'flits.order_contact_us.order_and_shipping_related_reason' | t }}">{{ 'flits.order_contact_us.order_and_shipping_related_reason' | t }}</option>
                <option value="{{ 'flits.order_contact_us.return_and_exchange_related_reason' | t }}">{{ 'flits.order_contact_us.return_and_exchange_related_reason' | t }}</option>
                <option value="{{ 'flits.order_contact_us.product_related_reason' | t }}">{{ 'flits.order_contact_us.product_related_reason' | t }}</option>
                <option value="{{ 'flits.order_contact_us.price_related_reason' | t }}">{{ 'flits.order_contact_us.price_related_reason' | t }}</option>
                <option value="{{ 'flits.order_contact_us.other_reason' | t }}">{{ 'flits.order_contact_us.other_reason' | t }}</option>
              </select>
              <div class="flits-select-arrow">
                <svg viewBox="0 0 24 24" width="14" height="14" stroke="currentColor" stroke-width="1.5" stroke-linecap="round" stroke-linejoin="round" fill="none" shape-rendering="geometricPrecision">
                  <path d="M6 9l6 6 6-6"></path>
                </svg> 
              </div>
            </div>
          </div>
          <small id="flits-alert-reason" class="flits-form-alert"></small>
        </div>
      </div>

      <div class="flits-row">
        <div class="flits-col-md-12 flits-popup-mb-15">
          <div class="flits-input-wrap">
            <label class="flits-input-label" for="{{ formId }}-message">{{ 'flits.order_contact_us.message_label' | t }} <span class="flits-requird-sign" aria-hidden="true">*</span></label>
            <textarea rows="5" maxlength="250" class="flits-form-message" id="{{ formId }}-message"
                      placeholder="{{ 'flits.order_contact_us.message_placeholder' | t }}"
                      name="contact[{{ 'flits.order_contact_us.message_label' | t }}]"></textarea>
            <span id="flits-msg-count" class="flits-text-count">0 / 250</span>
          </div>
          <small id="flits-alert-message" class="flits-form-alert"></small>
        </div>
      </div>

      <div class="flits-row">
        <div class="flits-col-sm-12 flits-col-md-12 flits-popup-mb-15">
          <div class="flits-input-wrap flits-input-icon-wrap">
            <label class="flits-input-label" for="{{ formId }}-url">{{ 'flits.order_contact_us.attachment_link_label' | t }}</label>
            <div class="flits-input-icon">
              <svg viewBox="0 0 24 24" width="18" height="18" stroke="currentColor" stroke-width="2" fill="none" stroke-linecap="round" stroke-linejoin="round" class="css-i6dzq1"><path d="M10 13a5 5 0 0 0 7.54.54l3-3a5 5 0 0 0-7.07-7.07l-1.72 1.71"></path><path d="M14 11a5 5 0 0 0-7.54-.54l-3 3a5 5 0 0 0 7.07 7.07l1.71-1.71"></path></svg>
            </div>
            <input type="text" class="flits-input flits-form-url" id="{{ formId }}-url"
                   placeholder="{{ 'flits.order_contact_us.attachment_link_placeholder' | t }}"
                   name="contact[{{ 'flits.order_contact_us.attachment_link_label' | t }}]"
                   value="{% if form[url] %}{{ form[url] }}{% endif %}">
          </div>
          <small id="flits-alert-url" class="flits-form-alert"></small>
        </div>
      </div>

      <p class="flits-model-footer-text">{{ 'flits.order_contact_us.footer_text' | t }}</p>
      <button class="flits-input" id="flits-contactus-submit" type="button">
        {{ 'flits.order_contact_us.submit_button' | t }}
      </button>
      {% endform %}

      <!-- Actual form which is used for submit -->
      {%- assign flitsFormId = 'contact-form' -%}
      {% form 'contact', id: flitsFormId %}

      <!-- Mail content headline -->
      <input type="hidden" name="contact[Message received from Contact Us form of Customer Account Page]"/>	

      <!-- Reason To Contact -->
      <input type="hidden" id="{{ flitsFormId }}-customer-reason" name="contact[{{ 'flits.order_contact_us.reason_label' | t }}]" value=""/>

      <!-- Customer Name -->
      <input type="hidden" id="{{ flitsFormId }}-customer-name" name="contact[{{ 'flits.order_contact_us.customer_name_label' | t }}]" value=""/>

      <!-- Customer Link hidden field -->
      <input type="hidden" id="{{ flitsFormId }}-customer-link" name="contact[{{ 'flits.order_contact_us.customer_link_label' | t }}]" value=""/>

      <!-- Email -->
      <input type="hidden" id="{{ flitsFormId }}-customer-email" name="contact[email]" value=""/>

      <!-- Phone Number -->
      <input type="hidden" id="{{ flitsFormId }}-customer-phone" name="contact[{{ 'flits.order_contact_us.phone_number_label' | t }}]" value=""/>

      <!-- Order Link hidden field -->
      <input type="hidden" id="{{ flitsFormId }}-order-link" name="contact[{{ 'flits.order_contact_us.order_link_label' | t }}]" value=""/>

      <!-- Order Date hidden field -->
      <input type="hidden" id="{{ flitsFormId }}-order-date" name="contact[{{ 'flits.order_contact_us.order_date_label' | t }}]" value=""/>

      <!-- Order Number hidden field -->
      <input type="hidden" id="{{ flitsFormId }}-order-number" name="contact[{{ 'flits.order_contact_us.order_number_label' | t }}]" value=""/>

      <!-- Order Name hidden field -->
      <input type="hidden" id="{{ flitsFormId }}-order-name" name="contact[{{ 'flits.order_contact_us.order_name_label' | t }}]" value=""/>

      <!-- Order Total Price hidden field -->
      <input type="hidden" id="{{ flitsFormId }}-order-total" name="contact[{{ 'flits.order_contact_us.order_total_price_label' | t }}]" value=""/>

      <!-- Order Payment Status hidden field -->
      <input type="hidden" id="{{ flitsFormId }}-order-payment-status" name="contact[{{ 'flits.order_contact_us.order_payment_status_label' | t }}]" value=""/>

      <!-- Order Fulfillment Status hidden field -->
      <input type="hidden" id="{{ flitsFormId }}-order-fulfillment-status" name="contact[{{ 'flits.order_contact_us.order_fulfillment_status_label' | t }}]" value=""/>

      <!-- Message -->
      <input type="hidden" id="{{ flitsFormId }}-customer-message" name="contact[{{ 'flits.order_contact_us.message_label' | t }}]" value=""/>

      <!-- Attachment Link -->
      <input type="hidden" id="{{ flitsFormId }}-customer-url" name="contact[{{ 'flits.order_contact_us.attachment_link_label' | t }}]" value=""/>
      {% endform %}

    </div>
  </div>
</div>
