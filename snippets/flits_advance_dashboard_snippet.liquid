
{% case flits_advance_dashboard_snippet  %}
{% when  'account_page_data' %}
<div class="flits-hidden flits-top-products-from-orders-extra-data">
  <style></style>
  <div class="flits-anchor">
    <a disabled data-type="flits" data-href="#flits-page-top-ordered-products" data-toggle='page' class="flits-list-group-item flits-bg-theme">{{ 'flits.navigation.top_ordered_products' | t }}</a>
  </div>
  <div class="flits-div">
    <div id="flits-page-top-ordered-products" data-type="page" class="flits-hidden">
      <div class="flits-page-heading">
        <span class="flits-back-icon flits-return-to-menu">
         {{ 'flits.buttons.back' | t }}
        </span>
        <span class="flits-heading">
          {{ 'flits.heading.top_ordered_products' | t }}
        </span>
      </div>
      <div class="flits-top-products-empty" style="display:none;">
        <div class="flits-empty-order-icon flits-text-center">
          <svg version="1.1" id="Capa_1" xmlns="http://www.w3.org/2000/svg" xmlns:xlink="http://www.w3.org/1999/xlink" x="0px" y="0px"
